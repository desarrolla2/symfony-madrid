<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="rss_client.class">Desarrolla2\Bundle\RSSClientBundle\Service\RSSClient</parameter>
    </parameters>

    <services>
        <service id="d2.client.rss" class="%rss_client.class%">
            <argument>%d2.client.rss.feeds%</argument>
        </service>
    </services>
</container>
